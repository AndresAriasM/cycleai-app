{
  "name": "cycleai-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sass:watch": "sass --watch src/styles:src/styles/compiled --style=expanded --source-map",
    "sass:build": "sass src/styles/main.scss:src/styles/compiled/main.css --style=compressed --no-source-map",
    "sass:dev": "sass src/styles/main.scss:src/styles/compiled/main.css --style=expanded --source-map",
    "css:minify": "cleancss -o dist/assets/main.min.css src/styles/compiled/main.css",
    "build:styles": "npm run sass:build && npm run css:minify",
    "build:full": "npm run build:styles && npm run build"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "clean-css-cli": "^5.6.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "sass": "^1.70.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}